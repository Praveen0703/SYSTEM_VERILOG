// Code your design here
module operators;
  
  logic [3:0]a,b;
  logic  c;
  
  initial begin
    a=4'b101x;
    b=4'b1xz1;
    
    //equality operator
    
    $display("a=%b",a);
    $display("b=%b",b);
    $display("c=%b",c);
    
    
 //logical equality
    c=(a==b);
    $display("output of logical operator a==b=%b",c);
    
   //logical inequality
    c=(a!=b);
    $display("output of logical inequality operator a=!b",c);
    
    //case euqlity
    c=(a===b);
    $display("output of case equality operator a===b",c);
    
    //case inequality
    c=(a!==b);
    $display("output of case inequality operator a==!b",c);
    
    
  end
endmodule

output= a=101x
# b=1xz1
# c=x
# output of logical operator a==b=x
# output of logical inequality operator a=!bx
# output of case equality operator a===b0
# output of case inequality operator a==!b1
